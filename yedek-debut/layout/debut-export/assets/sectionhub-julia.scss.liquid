          .irs .irs-from,.irs .irs-max,.irs .irs-min,.irs .irs-single,.irs .irs-to {
              top:4px
          }
      }

      @media (max-width: 1024px) {
          .irs .irs-from,.irs .irs-max,.irs .irs-min,.irs .irs-single,.irs .irs-to {
              top:2px
          }
      }

      .irs .irs-from::after,.irs .irs-single::after,.irs .irs-to::after {
          display: none
      }

      .irs .irs-slider {
          border-radius: 50%;
          border: solid 1px transparent;
          cursor: pointer
      }

      @media (min-width: 1025px) {
          .irs .irs-slider {
              width:10px;
              height: 10px;
              top: 21px
          }
      }

      @media (max-width: 1024px) {
          .irs .irs-slider {
              width:14px;
              height: 14px;
              top: 19px
          }
      }      
      .irs .irs-line {
          background-color: #e5e5e5
      }

      .irs .irs-bar {
          background-color: {{ settings.sh_header_color }}
      }

      .irs .irs-min,.irs .irs-max,.irs .irs-from,.irs .irs-to,.irs .irs-single {
          background-color: transparent;
          color: {{ settings.sh_paragraph_color }}
      }

      .irs .irs-slider {
          background-color: {{ settings.sh_header_color }};
          border-color: {{ settings.sh_border_color }}
      }

      .irs .irs-slider.state_hover {
          background-color: #fff
      }

      @media (min-width: 1025px) {
          .irs .irs-slider:hover {
              background-color:#fff
          }
      }
    }
  }
}

