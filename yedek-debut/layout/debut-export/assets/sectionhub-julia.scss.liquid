$transition: .3s all ease-in-out;

@mixin placeholder($property, $value) {
    &:placeholder {
        #{$property}: $value;
    }
    &::placeholder {
        #{$property}: $value;
    }
    &::-webkit-input-placeholder {
        #{$property}: $value;
    }
    &::-moz-placeholder {
        #{$property}: $value;
    }
    &:-moz-placeholder {
        #{$property}: $value;
    }
    &:-ms-input-placeholder {
     	#{$property}: $value;
    }     
}

@mixin sh_control {
  padding: 9px 20px;
  border: 1px solid {{ settings.sh_border_color }};
  background-image: none !important;
  @include placeholder(color, #aaa);
}

@mixin sh_button {
  @include sh_control;
  background-color: {{ settings.sh_header_color }};
  color: white;
  transition: $transition;
  display: inline-block;
  &:hover {
    background-color: white;
    color: {{ settings.sh_header_color }};
  }
}

@mixin sh_inverted_button {
  @include sh_control;
  background-color: white;
  border-color: white;
  color: {{ settings.sh_header_color }};
  transition: $transition;
  display: inline-block;
  &:hover {
    background-color: {{ settings.sh_brand_color }};
    border-color: {{ settings.sh_brand_color }};
    color: white;
  }
}

@mixin sh_text_button {  
  font-weight: 600;
  font-size: 15px;
  letter-spacing: 0.5px;
  display: inline-flex;
  align-items: center;
  color: {{ settings.sh_header_color }};
  .sh-text {
    margin-right: 5px;
  }                
  .sh-icon {
    font-size: 0;
    svg {
      width: 20px;
      height: 20px;
    }
  }
  border-bottom: 1px solid transparent;
  position: relative;
  &::before {
    content: '';
    position: absolute;
    transition: $transition;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid {{ settings.sh_header_color }};
    opacity: 0;
    transform: translateY(5px);
  }
  &:hover {
    &::before {
      opacity: 1;
      transform: translateY(0);
    }
  }            
}

@mixin sh_stickers {
  .sh-sale, .sh-new, .sh-sold-out {
    position: absolute;
    padding: 5px 10px;
    background-color: {{settings.sh_brand_color}};
    color: white;
    font-weight: 600;
    left: 10px;
    top: 10px;
    z-index: 1;
    font-size: 11px;
  }
}

@mixin sh_columns {
  .sh-columns {
    display: flex;
    flex-wrap: wrap;
    margin-right: -15px;
    margin-left: -15px;
    .sh-col-1, .sh-col-2, .sh-col-3, .sh-col-4, .sh-col-5, .sh-col-6, .sh-col-7, .sh-col-8, .sh-col-9, .sh-col-10, .sh-col-11, .sh-col-12 {
      padding-right: 15px;
      padding-left: 15px;
    }
    .sh-col {
      flex-basis: 0;
      flex-grow: 1;
      max-width: 100%
    }

    .sh-col-auto {
      flex: 0 0 auto;
      width: auto;
      max-width: none
    }

    .sh-col-1 {
      flex: 0 0 8.33333%;
      max-width: 8.33333%
    }

    .sh-col-2 {
      flex: 0 0 16.66667%;
      max-width: 16.66667%
    }

    .sh-col-3 {
      flex: 0 0 25%;
      max-width: 25%
    }

    .sh-col-4 {
      flex: 0 0 33.33333%;
      max-width: 33.33333%
    }

    .sh-col-5 {
      flex: 0 0 41.66667%;
      max-width: 41.66667%
    }

    .sh-col-6 {
      flex: 0 0 50%;
      max-width: 50%
    }

    .sh-col-7 {
      flex: 0 0 58.33333%;
      max-width: 58.33333%
    }

    .sh-col-8 {
      flex: 0 0 66.66667%;
      max-width: 66.66667%
    }

    .sh-col-9 {
      flex: 0 0 75%;
      max-width: 75%
    }

    .sh-col-10 {
      flex: 0 0 83.33333%;
      max-width: 83.33333%
    }

    .sh-col-11 {
      flex: 0 0 91.66667%;
      max-width: 91.66667%
    }

    .sh-col-12 {
      flex: 0 0 100%;
      max-width: 100%
    }
  }
}

@mixin sh_magnific_popup {
  .sh-magnific-popup {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    color: {{ settings.sh_header_color }};
    background-color: white;
    border-radius: 50%;
    padding: 20px;
    display: inline-block;
    font-size: 0;
    transition: $transition;
    .sh-icon {
      display: inline-block;
      padding-left: 3px;
      svg {
        width: 20px;
        height: 20px;
      }
    }
    &:hover {
      color: white;
      background-color: {{ settings.sh_brand_color }};
    }
    &::after {
      content: '';
      border-color: white;
      width: 100%;
      height: 100%;
      border-radius: 50px;
      position: absolute;
      background-color: rgba(255, 255, 255, 0.2);
      top: 0;
      left: 0;
      opacity: 1;
      animation-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
      animation-duration: 1.2s;
      animation-name: sh_video_scale;
      animation-iteration-count: infinite;
      transition: all cubic-bezier(0.4, 0, 0.2, 1) 0.25s; 
    }                
  }
}

